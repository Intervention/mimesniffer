{
    "name": "intervention/mimesniffer",
    "description": "PHP MIME Type Sniffer",
    "homepage": "http://intervention.io/",
    "keywords": ["mime", "type", "sniffer", "detector"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Vogel",
            "email": "oliver@olivervogel.com",
            "homepage": "https://olivervogel.com/"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^0.12.11"
    },
    "autoload": {
        "psr-4": {
            "Intervention\\MimeSniffer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Intervention\\MimeSniffer\\Test\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
